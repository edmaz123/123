let chart c = int <| c.ToString()


    

let rec next n =
    match n with
    | 9 :: t -> 0 :: next t
    | h :: t -> h + 1 :: t
    | [] -> [1]



let folder acc d = d.ToString() + acc
let r2 =
    stdin.ReadLine()
    |>List.ofSeq
    |>List.map chart
    |>List.rev
    |>next
    |>List.fold folder ""
    |>printfn "%s"
